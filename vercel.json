{
  "headers": [
    {
      "source": "/apk/(.*\\.apk)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/vnd.android.package-archive"
        },
        {
          "key": "Content-Disposition",
          "value": "attachment; filename=\"$1\""
        }
      ]
    }
  ]
}